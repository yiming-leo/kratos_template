# 集群所有节点ip，有几台填几台
k8s_ips=(
192.168.3.190
192.168.3.191
192.168.3.192
192.168.3.193
192.168.3.194
)

# master节点ip
master_ip="192.168.3.190"

# harbor的ip，确保和development.yaml内的镜像地址的harbor地址一致
harbor_ip="192.168.2.14:214"

# 即将部署的命名空间，确保和development.yaml、service.yaml内的命名空间一致
namespace="kratos-test"

# replicas number，确保和development.yaml内的主从数量一致
replicas=2

# pod name，确保和development.yaml内的pod名字一致
pod_name="test-server1-deployment"

# selector name，确保和development.yaml与service.yaml内的app名字一致
selector_app_name="test-server1"

# image name，确保和development.yaml内的镜像地址一致
image_name="192.168.2.14:214/kratos_test/test_server1:v1.0"

# image pull secrets，确保和自己在k8s内生成的密钥名字一致
image_pull_secrets="myregistrykey"

# service type，确保和service.yaml的端口类型一致
service_type="NodePort"